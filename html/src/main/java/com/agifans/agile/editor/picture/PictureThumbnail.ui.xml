<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:picture='urn:import:com.agifans.agile.editor.picture'>

  <ui:with type="com.agifans.agile.editor.picture.PictureThumbnailData" field="pictureThumbnailData"></ui:with>

  <g:HTMLPanel>
  
	<div>
	
	    <g:HTMLPanel ui:field="thumbnailPanel">

	        <div class="sprite-selector-item_sprite-image-outer">
	            <div class="sprite-selector-item_sprite-image-inner">
	                <g:Image ui:field="pictureImage" />
	            </div>
	        </div>
	        
	        <div class="sprite-selector-item_sprite-info">
	            <div class="sprite-selector-item_sprite-name">Picture <ui:text from="{pictureThumbnailData.getPictureNumber}" /></div>
	        </div>
	
	    </g:HTMLPanel>
	    
	</div>

  </g:HTMLPanel>

</ui:UiBinder>